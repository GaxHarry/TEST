 setInterval(() => {\r\n      this.lock = this.beforeMouse.x === this.mouse.x && this.beforeMouse.y === this.mouse.y;\r\n      this.beforeMouse.x = this.mouse.x;\r\n      this.beforeMouse.y = this.mouse.y;\r\n    }, 400);\r\n    setInterval(() => {\r\n      if (!this.lock) {\r\n        return;\r\n      }\r\n\r\n      this.page.current += 1;\r\n      if ((this.page.current - 1) * this.page.size > this.page.total) {\r\n        this.page.current = 1;\r\n      }\r\n      this.onLoad();\r\n    }, 3000)\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.select-item {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  color: #FFFFFF;\r\n  padding: 0 30px;\r\n  margin: 10px 20px 0 20px;\r\n}\r\n</style>"],"mappings":"AAkCA,OAAAA,SAAA;AACA,OAAAC,KAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAH;EAAA;EACAI,KAAA;IACA;MACAA,IAAA;MACAC,IAAA;QACAC,OAAA;QACAC,IAAA;MACA;MACAC,IAAA;MACAC,WAAA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA;MACAX,KAAA,CAAAY,KAAA,iBAAAR,IAAA,CAAAC,OAAA,OAAAD,IAAA,CAAAE,IAAA,EAAAO,IAAA,CAAAC,GAAA;QACA,IAAAX,IAAA,GAAAW,GAAA,CAAAX,IAAA;QACA,KAAAC,IAAA,CAAAW,KAAA,GAAAZ,IAAA,CAAAY,KAAA;QACA,KAAAZ,IAAA,GAAAA,IAAA,CAAAa,OAAA;MACA;IACA;EACA;EACAC,QAAA;IACAC,QAAA,CAAAC,gBAAA,cAAAC,KAAA;MACA,KAAAX,KAAA,CAAAY,CAAA,GAAAD,KAAA,CAAAE,OAAA;MACA,KAAAb,KAAA,CAAAc,CAAA,GAAAH,KAAA,CAAAI,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAd,MAAA;IACAe,WAAA;MACA,KAAAnB,IAAA,QAAAC,WAAA,CAAAa,CAAA,UAAAZ,KAAA,CAAAY,CAAA,SAAAb,WAAA,CAAAe,CAAA,UAAAd,KAAA,CAAAc,CAAA;MACA,KAAAf,WAAA,CAAAa,CAAA,QAAAZ,KAAA,CAAAY,CAAA;MACA,KAAAb,WAAA,CAAAe,CAAA,QAAAd,KAAA,CAAAc,CAAA;IACA;IACAG,WAAA;MACA,UAAAnB,IAAA;QACA;MACA;MAEA,KAAAH,IAAA,CAAAC,OAAA;MACA,UAAAD,IAAA,CAAAC,OAAA,aAAAD,IAAA,CAAAE,IAAA,QAAAF,IAAA,CAAAW,KAAA;QACA,KAAAX,IAAA,CAAAC,OAAA;MACA;MACA,KAAAM,MAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}